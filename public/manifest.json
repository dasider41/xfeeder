{
  "short_name": "xFeeder",
  "manifest_version": 2,
  "name": "xFeeder extension",
  "description": "This is an RSS feeder for Chrome extension",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["<all_urls>", "http://*/*", "https://*/*", "alarms", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
