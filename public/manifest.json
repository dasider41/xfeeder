{
  "short_name": "xFeeder",
  "manifest_version": 2,
  "name": "xFeeder extension",
  "description": "This is an RSS feeder for Chrome extension",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "alarms"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
